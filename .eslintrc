{
	"env": {
	    "browser": true,
	    "node": true,
	    "es6": true
	  },
  	"globals": {
	    "nconf": true
	  },
  	"parser": "babel-eslint",
	"plugins": [
	    "react"
	  ],
  	"extends": ["react-app", "plugin:react/recommended"],
  	"rules": {
  	    
  	    "indent": [1, 2], 
  	    "comma-dangle": 2,
  	    "comma-spacing": [1, {"before": false, "after": true}], 
        "comma-style": [1, "last"],
        "semi": [1, "always"],
        "quotes": [ 1, "single", "avoid-escape" ],
        "react/jsx-curly-spacing": [ 2, "always" ],
        "react/jsx-indent": [ 1, 2 ],
        "no-undef": [ 1 ],
  	  }
}